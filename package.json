{
  "name": "adnot-beport-small",
  "version": "1.0.1",
  "description": "Notations used in Antimatter Dimensions, built for break_eternity.js. This set of notations is specifically built for numbers less than 1e9e15",
  "scripts": {
    "build": "npm run build:ad && npm run build:community",
    "build:ad": "bili --config bili.config.base.ts",
    "build:community": "bili --config bili.config.community.ts",
    "lint": "eslint .",
    "test": "jest"
  },
  "main": "dist/ad-notations.umd.js",
  "module": "dist/ad-notations.esm.js",
  "unpkg": "dist/ad-notations.umd.js",
  "jsdelivr": "dist/ad-notations.umd.js",
  "keywords": [
    "notations",
    "numbers"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "docs",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/HexaVault/BEnotations-Pre.git"
  },
  "bugs": {
    "url": "https://github.com/HexaVault/BEnotations-Pre/issues"
  },
  "homepage": "https://github.com/HexaVault/BEnotations-Pre",
  "devDependencies": {

    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "bili": "^5.0.2",
    "eslint": "^7.0.0",
    "jest": "^26.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "break_eternity.js": "^1.3.1",
    "jsmin": "^1.0.1",
    "tslib": "^2.0.0"
  }
}
